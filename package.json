{
  "name": "electron-webrtc-webtorrent",
  "version": "0.3.5",
  "description": "Use WebRTC in Node.js via a hidden Electron process",
  "main": "index.js",
  "browser": "browser.js",
  "scripts": {
    "test": "standard && tap test/*.js -t150 -Rspec --cov",
    "test-tap": "tap test/*.js -Rtap",
    "test-lcov": "mkdir -p coverage && tap test/*.js -Rsilent --coverage-report=text-lcov > coverage/lcov.info"
  },
  "engines": {
    "node": ">=4"
  },
  "keywords": [
    "webrtc",
    "electron",
    "webtorrent"
  ],
  "author": "Drakonkat <adamo.mazzocchetti@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.2.0",
    "electron": "^25.1.1",
    "get-browser-rtc": "^1.1.0",
    "hat": "^0.0.3",
    "ndjson": "^1.5.0"
  },
  "devDependencies": {
    "simple-peer": "^9.11.1",
    "standard": "^17.1.0",
    "string-to-stream": "^1.0.1",
    "tap": "^10.7.0"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drakonkat/electron-webrtc.git"
  },
  "bugs": {
    "url": "https://github.com/drakonkat/electron-webrtc/issues"
  },
  "homepage": "https://github.com/drakonkat/electron-webrtc#readme"
}
